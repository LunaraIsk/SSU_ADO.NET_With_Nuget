    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ApplicationSystem</DataSourceName>
        <CommandText>SELECT CAST('2020-05-12' AS date) as DateOfCreation, CAST(1 AS int) as Id_Inventory, 2 as Amount, 'Изящные стулья с мягкими элементами' as Comment
UNION SELECT CAST('2020-02-03' AS date) as DateOfCreation, CAST(2 AS int) as Id_Inventory, 2 as Amount, 'Белый стол' as Comment
UNION SELECT CAST('2020-06-30' AS date) as DateOfCreation, CAST(3 AS int) as Id_Inventory, 2 as Amount, 'Цифровой калькулятор' as Comment
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DateOfCreation">
          <DataField>DateOfCreation</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Id_Inventory">
